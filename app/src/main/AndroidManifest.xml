        </activity>
        <activity
            android:name=".HomeActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".CreateWheelsActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".InfoCreationActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".GetWheelsActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".InfoGetWheelsActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".AccountInfoActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".ReservationInfo"
            android:exported="false">
        </activity>
    </application>

</manifest>